    "sanity-generate-types": "npx sanity typegen generate",
    "update-sanity-types": "npm run sanity-extract-schema && npm run sanity-generate-types",
    "update-types": "npm run update-supabase-types && npm run update-sanity-types",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
