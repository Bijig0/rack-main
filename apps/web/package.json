    "type-check": "tsc --pretty --noEmit",
        "test": "npx vitest",
    "test:ci": "jest --ci",
    "analyze": "ANALYZE=true pnpm build",
    "update-supabase-types": "npx supabase gen types typescript --project-id agslweirgmorisihaqbz --schema public > src/app/types/supabase.ts",
